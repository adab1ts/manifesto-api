{
  "name": "Signer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "group": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "scope": {
    "order": "name ASC",
    "limit": 100
  },
  "hidden": ["id", "email", "created_at"],
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
